{
  "name": "selenium-thinkingtester",
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "test": "mocha --timeout 80000 \"src/tests/**/*.spec.js\" --reporter mochawesome --reporter-options reportDir=reports,reportFilename=index,quiet=true,html=true,json=false",
    "test:headful": "HEADFUL=1 npm test",
    "posttest": "node scripts/open-report.js"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "selenium-webdriver": "^4.22.0"
  },
  "devDependencies": {
    "chai": "^4.4.1",
    "mocha": "^10.4.0",
    "mochawesome": "^7.1.3"
  }
}
